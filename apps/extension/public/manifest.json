{
  "name": "Productivitia",
  "description": "Improving productivity through a startpage that gamifies your life",
  "version": "1.0.0",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "startpage.html"
  },
  "chrome_settings_overrides": {
    "homepage": "https://productivitia.netlify.app/app"
  },
  "icons": {
    "500": "icon.png"
  },
  "omnibox": {
    "keyword": "productivitia"
  },
  "permissions": ["tabs", "history"],
  "browser_specific_settings": {
    "gecko": {
      "id": "productivitia@netlify.com",
      "strict_min_version": "55.0"
    }
  }
}
